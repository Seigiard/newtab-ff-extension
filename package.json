{
  "name": "newtab-ff-extension",
  "version": "1.1.0",
  "description": "A Firefox extension that puts your homepage on new tabs, without stealing keyboard focus from the address bar.",
  "scripts": {
    "start:firefox": "web-ext run --source-dir ./src/",
    "start:watefox": "web-ext run -f /Applications/Waterfox.app/Contents/MacOS/waterfox-bin --source-dir ./src/",
    "build": "web-ext build --source-dir ./src/"
  },
  "author": "Andrej Borisenko & Leigh Brenecki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Seigiard/newtab-ff-extension/issues"
  },
  "homepage": "https://github.com/Seigiard/newtab-ff-extension",
  "dependencies": {
    "web-ext": "^7.9.0"
  }
}